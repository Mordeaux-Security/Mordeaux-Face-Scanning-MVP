# Mordeaux Face Scanning MVP - GPU Optimized Environment Variables

# Database Configuration
POSTGRES_DB=mordeaux
POSTGRES_USER=mordeaux
POSTGRES_PASSWORD=mordeaux123

# S3/MinIO Configuration
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_ENDPOINT=http://minio:9000
S3_REGION=us-east-1
S3_BUCKET_RAW=raw-images
S3_BUCKET_THUMBS=thumbnails
S3_BUCKET_AUDIT=audit-logs
S3_USE_SSL=false

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=info
ASGI_WORKERS=2

# API Configuration
VITE_API_BASE=http://localhost:8000

# Nginx Configuration
NGINX_MAX_BODY=50m

# Vector Database (Qdrant)
VECTOR_INDEX=faces_v1

# Optional: Pinecone Configuration (leave empty if not using Pinecone)
PINECONE_API_KEY=
PINECONE_INDEX=faces_v1

# GPU Configuration
CUDA_VISIBLE_DEVICES=0
ONNX_PROVIDERS=CUDAExecutionProvider,CPUExecutionProvider

# Timezone
TZ=America/Los_Angeles
